          "name": "How Internet Works",
          "id": "how-internet-works",
          "meta": {
            "description": "Learn about packets, routers, TCP/IP protocols",
            "duration": "3-4 days",
            "level": "beginner",
            "topics": ["TCP/IP", "Packets", "Routers", "ISP"],
            "resources": [{"name": "Khan Academy - Internet 101", "url": "https://www.khanacademy.org/computing/computers-and-internet"}]
