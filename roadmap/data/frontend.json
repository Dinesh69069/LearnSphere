      "id": "internet-fundamentals",
      "meta": {
        "description": "Understanding how the internet and web browsers work",
        "duration": "2-3 weeks",
        "level": "beginner",
        "topics": ["HTTP/HTTPS", "DNS", "Domain Names", "Hosting", "Browsers"],
        "resources": [
          {"name": "How the Internet Works", "url": "https://developer.mozilla.org/en-US/docs/Learn/Common_questions/How_does_the_Internet_work"}
