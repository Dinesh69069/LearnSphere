            "level": "beginner",
            "topics": ["GET/POST", "Status Codes", "Headers", "Cookies", "SSL/TLS"],
            "resources": [{"name": "HTTP Guide", "url": "https://developer.mozilla.org/en-US/docs/Web/HTTP"}]
          }
        },
        {
          "name": "DNS & Domain Names",
          "id": "dns-domains",
          "meta": {
            "description": "How DNS resolution works, domain registration",
            "duration": "3-4 days",
            "level": "beginner",
            "topics": ["DNS Lookup", "A Records", "CNAME", "Name Servers"],
            "resources": [{"name": "How DNS Works", "url": "https://howdns.works/"}]
          }
        },
        {
          "name": "Browsers & Rendering",
          "id": "browsers",
          "meta": {
            "description": "How browsers parse and render web pages",
            "duration": "1 week",
            "level": "beginner",
            "topics": ["DOM", "CSSOM", "Render Tree", "Painting", "Compositing"],
            "resources": [{"name": "How Browsers Work", "url": "https://web.dev/howbrowserswork/"}]
          }
